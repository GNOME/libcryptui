#: ../src/seahorse-key-manager-store.c:943
#: ../src/seahorse-key-properties.c:871
msgid "Name"
msgstr "Név"

#: ../libcryptui/cryptui-key-list.c:120
#: ../src/seahorse-key-manager-store.c:955
#: ../src/seahorse-key-properties.c:1725
msgid "Key ID"
msgstr "Kulcsazonosító"

#: ../libseahorse/seahorse-add-keyring.glade.h:1
msgid "Add Password Keyring"
msgstr "Jelszókulcstartó hozzáadása"

#: ../libseahorse/seahorse-add-keyring.glade.h:2
msgid "New Keyring Name:"
msgstr "Új kulcstartónév:"

#: ../libseahorse/seahorse-add-keyring.glade.h:3
msgid ""
"Please choose a name for the new keyring. You will be prompted for an unlock "
"password."
msgstr "Válassza ki az új kulcstartó nevét. A program bekéri a feloldási jelszót."

#: ../libseahorse/seahorse-add-keyring.glade.h:4
#: ../libseahorse/seahorse-add-keyserver.glade.h:5
#: ../src/seahorse-ssh-upload.glade.h:3
msgid "The host name or address of the server."
msgstr "A kiszolgáló neve vagy címe."

#: ../libseahorse/seahorse-add-keyserver.glade.h:1
msgid ":"
msgstr ":"

#: ../libseahorse/seahorse-add-keyserver.glade.h:2
msgid "Add Key Server"
msgstr "Kulcskiszolgáló hozzáadása"

#: ../libseahorse/seahorse-add-keyserver.glade.h:3
msgid "Host:"
msgstr "Gép:"

#: ../libseahorse/seahorse-add-keyserver.glade.h:4
msgid "Key Server Type:"
msgstr "Kulcskiszolgáló típusa:"

#: ../libseahorse/seahorse-add-keyserver.glade.h:6
msgid "The port to access the server on."
msgstr "A port, melyen a kiszolgáló elérhető."

#: ../libseahorse/seahorse-hkp-source.c:323
#, c-format
msgid "Search was not specific enough. Server '%s' found too many keys."
msgstr ""
"A keresés nem volt elég egyedi. A(z) „%s” kiszolgáló túl sok kulcsot "
"talált."

#: ../libseahorse/seahorse-hkp-source.c:325
#, c-format
msgid "Couldn't communicate with server '%s': %s"
msgstr "Nem lehet kommunikálni a(z) „%s” kiszolgálóval: %s"

