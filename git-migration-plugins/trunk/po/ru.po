msgstr "Добавить сервер ключей"

#: ../libseahorse/seahorse-add-keyserver.glade.h:3
msgid "Host:"
msgstr "Узел:"

#: ../libseahorse/seahorse-add-keyserver.glade.h:4
msgid "Key Server Type:"
msgstr "Тип сервера ключей:"

#: ../libseahorse/seahorse-add-keyserver.glade.h:6
msgid "The port to access the server on."
msgstr "Порт для доступа к серверу."

#: ../libseahorse/seahorse-hkp-source.c:323
#, c-format
msgid "Search was not specific enough. Server '%s' found too many keys."
msgstr ""
"Параметры поиска были недостаточно определёнными. Сервер «%s» нашёл по ним "
"слишком много ключей."

#: ../libseahorse/seahorse-hkp-source.c:325
#, c-format
msgid "Couldn't communicate with server '%s': %s"
msgstr "Не удалось связаться с сервером «%s»: %s"

#: ../libseahorse/seahorse-hkp-source.c:521
msgid "Searching for keys..."
msgstr "Идёт поиск ключей..."

#: ../libseahorse/seahorse-hkp-source.c:592
msgid "Uploading keys..."
msgstr "Идёт загрузка ключей..."

#: ../libseahorse/seahorse-hkp-source.c:665
